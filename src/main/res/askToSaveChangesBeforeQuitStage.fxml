<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="100.0" prefWidth="605.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.github.malyszaryczlowiek.cpcdb.Controllers.AskToSaveChangesBeforeQuitController">
   <children>
      <Button fx:id="saveButton" layoutX="486.0" layoutY="56.0" mnemonicParsing="false" onAction="#onSaveButtonClicked" prefHeight="26.0" prefWidth="99.0" text="Save" />
      <Button layoutX="376.0" layoutY="56.0" mnemonicParsing="false" onAction="#onDoNotSaveButtonClicked" text="Do Not Save" />
      <Button layoutX="265.0" layoutY="56.0" mnemonicParsing="false" onAction="#onCancelButtonClicked" prefHeight="26.0" prefWidth="99.0" text="Cancel" />
      <Label layoutX="20.0" layoutY="19.0" prefHeight="26.0" prefWidth="565.0" text="There are still not saved changes. Do you want to save them? ">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
